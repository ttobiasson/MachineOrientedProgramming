.PHONY: clean All

All:
	@echo "----------Building project:[ autopong - Debug ]----------"
	@cd "autopong" && "$(MAKE)" -f  "autopong.mk" && "$(MAKE)" -f  "autopong.mk" PostBuild
clean:
	@echo "----------Cleaning project:[ autopong - Debug ]----------"
	@cd "autopong" && "$(MAKE)" -f  "autopong.mk" clean
